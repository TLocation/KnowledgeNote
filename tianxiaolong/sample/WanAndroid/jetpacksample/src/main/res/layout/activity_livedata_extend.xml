<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
<TextView
    android:text="姓名"
    android:id="@+id/extend_livedata_name"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"/>

    <TextView
        android:text="年龄"
        android:id="@+id/extend_livedata_age"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <TextView
        android:text="年龄状态"
        android:id="@+id/extend_livedata_age_state"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <TextView
        android:text="id"
        android:id="@+id/extend_livedata_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <CheckBox
        android:layout_marginTop="10dp"
        android:text="注册用户信息"
        android:id="@+id/extend_livedata_regist"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>


</LinearLayout>